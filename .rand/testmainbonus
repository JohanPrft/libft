#include "libft.h"

typedef struct s_list
{
	void			*content;
	struct s_list	*next;
}	t_list;

t_list	*initialisation(void)
{
	t_list	*list;
	int		*content;
	t_list	*next;

	list = malloc(sizeof(*list));
	*content = malloc(sizeof(*content));
	*next = malloc(sizeof(*next));
	if (!list || !content || !next)
		return (NULL);
}
